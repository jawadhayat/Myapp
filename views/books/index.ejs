<h1>Search Books</h1>
<form method="GET" action="/books">
<div>
    <label for="title">Title</label>
    <input type="text" name="title" value="<%= searchOption.title  %>" >
</div>
<div>
    <label for="publishedAfter">Published After</label>
    <input type="date" name="publishedAfter" value="<%= searchOption.publishedAfter  %>" >
</div>
<div>
    <label for="publishBefore">Published Before</label>
    <input type="date" name="publishedBefore" value="<%= searchOption.publishedBefore  %>" >
</div>
<button type="sumit">Search</button>
</form>
<br>
<div>
    <% books.forEach(book =>{ %>
        <img src="<%= book.coverImagePath %>" alt="" height="400" width="500" ><br><br><br>
    <% }); %>
</div>